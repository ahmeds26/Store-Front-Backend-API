{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "npx tsc",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "migrate": "db-migrate up", 
    "testdb-up": "db-migrate --env test up",
    "testdb-reset": "db-migrate --env test reset",
    "test": "set ENV=test&& npm run testdb-reset && npm run testdb-up && jasmine-ts",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint --ext .ts"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "supertest": "^6.2.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.7",
    "@types/prettier": "^2.4.4",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.9.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
